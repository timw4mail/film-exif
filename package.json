{
  "name": "film-exif-electron",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "exiftool-vendored": "^4.20.0",
    "inferno": "^5.0.1",
    "inferno-bootstrap": "^5.0.0",
    "inferno-router": "^5.0.1",
    "inferno-scripts": "6.3.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "electron": "^1.8.4",
    "electron-builder": "^20.8.1",
    "eslint": "^4.19.1",
    "eslint-config-happiness": "^10.2.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-inferno": "^7.7.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "foreman": "^2.0.0",
    "husky": "^0.14.3"
  },
  "scripts": {
    "build": "inferno-scripts build",
    "dist": "yarn run build && build",
    "eject": "inferno-scripts eject",
    "electron-start": "node src/electron/wait-inferno",
    "electron": "electron .",
    "fix": "set NODE_DIR=src/ eslint --fix src/**/*.js",
    "lint": "set NODE_DIR=src/ eslint src/**/*.js",
    "pack": "build --dir",
    "postinstall": "set NODE_DIR=src/ electron-builder install-app-deps",
    "precommit": "yarn lint",
    "react-start": "inferno-scripts start",
    "start": "nf start -p 3000",
    "test": "inferno-scripts test --env=jsdom"
  },
  "homepage": "./",
  "main": "src/electron/starter.js"
}
